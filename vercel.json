{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/cards",
      "destination": "/cards.html"
    },
    {
      "source": "/dashboard",
      "destination": "/Dashboard.html"
    },
    {
      "source": "/game",
      "destination": "/game.html"
    },
    {
      "source": "/raw-data",
      "destination": "/Raw_data.html"
    }
  ]
}
